    "import numpy as np\n",
    "import pandas as pd\n",
    "\n",
    "price = np.linspace(5, 50, 50)\n",
    "demand = 1000 - 20 * price + np.random.normal(0, 50, 50)\n",
    "\n",
    "profit = price * demand\n",
    "\n",
    "df = pd.DataFrame({\"price\": price, \"demand\": demand, \"profit\": profit})\n",
    "\n",
    "best = df.loc[df[\"profit\"].idxmax()]\n",
    "best\n"
   ]
  }
